@{
    ViewBag.Title = "Sql Report";
    ViewBag.ReportPageHeader = ViewBag.DisplayName;
    Layout = ViewExtensions2.TouchPointLayout();
}
<div class="report box box-responsive">
  <div class="box-content">
    <form action="@ViewBag.ExcelUrl" target="_blank" method="POST">
      <input type="hidden" name="orgIds" value="@ViewBag.OrgIds" />
      <button type="submit" class="btn btn-default" value="Submit"><i class="fa fa-file-excel-o"></i> Download as Excel</button>
    </form>
    <div class="table-responsive">
      @Html.Raw(ViewBag.Results)
    </div>
  </div>
</div>